  "devDependencies": {
    "@babel/core": "7.5.0",
    "@babel/preset-env": "7.5.0",
    "babel-loader": "8.0.6",
    "babel-polyfill": "6.26.0",
    "copy-webpack-plugin": "6.3.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsdoc": "30.5.1",
    "eslint-plugin-prefer-arrow": "1.2.2",
